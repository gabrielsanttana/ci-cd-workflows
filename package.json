  "main": "index.js",
  "repository": "git@github.com:gabrielsanttana/ci-cd-workflows.git",
  "author": "gabrielsanttana <gabriel.gsantana7@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage"
  },
  "dependencies": {
    "@types/jest": "^26.0.20",
    "eslint": "^7.20.0",
    "jest": "^26.6.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx,js,jsx}",
      "!<rootDir>/src/*.{ts,tsx}",
      "!<rootDir>/src/utils/**/*.{ts,tsx}",
      "!<rootDir>/src/store/**/*.{ts,tsx}",
      "*.{js, ts, tsx, jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    }
  }
}
